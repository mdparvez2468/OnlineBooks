<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shishubooks">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/parvez"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AccountActivity"></activity>
        <activity
            android:name=".TutorialsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".VideoLecturesActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".QuizActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Setting"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FeedBack"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AboutusActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BookActivity"
            android:parentActivityName=".ClassOneBooksActivity" />
        <activity
            android:name=".ClassOneBooksActivity"
            android:parentActivityName=".BookListActivity" />
        <activity
            android:name=".BookListActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>